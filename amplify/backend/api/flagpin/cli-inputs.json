{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "flagpin",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "AMAZON_COGNITO_USER_POOLS",
      "cognitoUserPoolId": "authflagpin"
    },
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Application"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Project"
        }
      ]
    }
  }
}